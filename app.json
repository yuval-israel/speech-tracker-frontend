{
  "expo": {
    "name": "SpeechTracker",
    "slug": "speech-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],
    "plugins": [
      [
        "expo-audio",
        {
          "microphonePermission": "Allow this app to access your microphone."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["RECORD_AUDIO"]
    }
  }
}
